{
  "name": "seneca-transport",
  "version": "1.2.0",
  "description": "Seneca transport",
  "main": "transport.js",
  "license": "MIT",
  "author": "Richard Rodger (http://richardrodger.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/senecajs/seneca-transport.git"
  },
  "keywords": [
    "seneca",
    "transport",
    "plugin"
  ],
  "scripts": {
    "test": "lab -v -L -P test -t 82 -m 8000 $LAB_FLAGS",
    "test:node6": "LAB_FLAGS='--leaks' && npm test",
    "test-report": "lab -v -L -P test -t 82 -m 5000 -r html > test/report.html; open test/report.html",
    "bench": "node bench",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "annotate": "docco transport.js -o doc; open doc/transport.html"
  },
  "dependencies": {
    "eraro": "0.4.x",
    "gex": "0.2.x",
    "jsonic": "0.2.x",
    "lodash": "4.13.x",
    "lru-cache": "4.0.x",
    "ndjson": "1.4.x",
    "nid": "0.3.2",
    "patrun": "0.5.x",
    "qs": "6.1.x",
    "reconnect-core": "1.2.x",
    "wreck": "6.3.x"
  },
  "devDependencies": {
    "async": "1.5.x",
    "bench": "0.3.x",
    "code": "1.x.x",
    "coveralls": "^2.11.9",
    "docco": "0.7.x",
    "eslint-config-seneca": "1.x.x",
    "eslint-plugin-standard": "1.x.x",
    "lab": "6.x.x",
    "seneca": "plugin",
    "seneca-entity": "1.0.x",
    "seneca-transport-test": "0.2.x"
  }
}
